<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- Cloud backup rules (Google Drive) -->
    <cloud-backup>
        <include domain="sharedpref" path="." />
    </cloud-backup>

    <!-- Device-to-device transfer rules -->
    <device-transfer>
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>
